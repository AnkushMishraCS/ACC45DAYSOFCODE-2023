//Check  if the alphabet entered is lowercase or uppercase.

  #include<stdio.h>

int main(){
char ch;
// 97-122 lowercase
printf("Enter the character\n");
scanf("%c",&ch);

if (ch>=97&&ch<=122)
printf("Your character is lowercase  ");
else 
printf("Your character is uppercase  ");


    return 0;
}
