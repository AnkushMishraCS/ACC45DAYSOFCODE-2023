// This is   print natural number program, using while loop.
#include<stdio.h>
int main(){
int a,b ;
a=0;
printf("Enter the number\n");
scanf("%d",&b);
do {
    printf("natural numbers are :%d\n",a+1);
    a++;
}while(a<b);


    return 0;
} 
