//check if the number is divisible by 97 or not?

  #include<stdio.h>
#include<math.h>

int main(){
int a, divisible;

printf("Enter a number \n ");
scanf("%d",&a);


divisible = a % 97;  

 
(divisible==0) ? printf("Yes your number is divisible by 97"):printf("Sorry your number is not divisible by 97 ");


 return 0;
}
