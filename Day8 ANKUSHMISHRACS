//This  program is about rating of a code.
#include<stdio.h>
int main(){
int rating;
printf("How many ratings will you give to my code? (1-5)\n");
scanf("%d",&rating);
switch(rating)
{case 1:
 printf("Your rating is 1 ");
 break;
case 2: 
printf("Your rating is 2 ");
break;
case 3:
 printf("Your rating is 3  ");
 break;
case 4:
 printf("Your rating is 4 ");
 break;
case 5:
 printf("Your rating is 5 ");
 break;
 deafault :
default :
 printf("Thankyou");


}


    return 0;
} 
